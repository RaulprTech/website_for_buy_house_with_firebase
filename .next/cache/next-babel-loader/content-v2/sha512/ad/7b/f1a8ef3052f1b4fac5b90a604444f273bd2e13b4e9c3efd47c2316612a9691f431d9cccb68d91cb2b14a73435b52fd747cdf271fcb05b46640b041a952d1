n                const data = snapshot.val()\n                for (let key in data) {\n                    let elems = {\n                        code: data[key]['code'],\n                        photo: data[key]['photo'],\n                    }\n                    booths.push(elems);\n                }\n            })\n        return booths;\n    }\n\n    res.statusCode = 200\n    res.json({ booths: getBooths })\n\n}"]},"metadata":{},"sourceType":"module"}